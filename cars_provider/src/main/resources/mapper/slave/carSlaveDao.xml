<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" " http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiyan.dao.slave.CarSlaveDao" >

    <resultMap id="car" type="com.xiyan.model.entrty.Car">
        <id property="carId" column="car_id"/>
        <result property="carLicense" column="car_license"/>
        <result property="carBrand" column="car_brand"/>
        <result property="carType" column="car_type"/>
        <result property="carPrice" column="car_price"/>
        <result property="inStore" column="in_store"/>
        <result property="carState" column="car_state"/>
        <result property="rentalPrice" column="rental_price"/>
        <result property="carPicture" column="car_picture" typeHandler="com.xiyan.model.typeHandler.CarsPicUrlTypeHandler"/>
        <result property="carQualifications" column="car_qualification" typeHandler="com.xiyan.model.typeHandler.CarQualTypeHandler"/>
        <result property="insuranceType" column="insurance_type" typeHandler="com.xiyan.model.typeHandler.ArrayListTypeHandler"/>
        <result property="aduitType" column="aduit_type"/>
        <result property="aduitId" column="aduit_id"/>
    </resultMap>

    <select id="listAllCar" resultMap="car">
        select * from car;
    </select>
</mapper>