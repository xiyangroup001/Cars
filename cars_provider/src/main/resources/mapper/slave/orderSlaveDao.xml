<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" " http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiyan.dao.slave.CarSlaveDao" >


    <resultMap id="order" type="com.xiyan.model.entrty.Order">
        <id property="orderId" column="order_id"/>
        <result property="userId" column="user_id"/>
        <result property="carId" column="car_id"/>
        <result property="orderGenerationTime" column="order_generation_time"/>
        <result property="startTime" column="start_time"/>
        <result property="endTime" column="end_time"/>
        <result property="takeCarShop" column="take_car_shop"/>
        <result property="returnCarShop" column="return_car_shop"/>
        <result property="prepayAmount" column="prepay_amount"/>
        <result property="totalAmount" column="total_amount"/>
        <result property="payType" column="pay_type"/>
        <result property="position" column="position" typeHandler="com.xiyan.model.typeHandler.PositionTypeHandler"/>
    </resultMap>

    <select id="listAllOrder" resultMap="order">
        select * from order;
    </select>
</mapper>