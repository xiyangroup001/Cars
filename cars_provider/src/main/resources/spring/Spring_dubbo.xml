<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="cars_provider" owner="programmer" organization="dubbox"/>
    <dubbo:registry id="cars-registry" protocol="zookeeper" address="${zookeeper.url}" check="false" timeout="3000"/>
    
    <dubbo:service interface="com.xiyan.service.UserService" class="com.xiyan.service.impl.UserServiceImpl"/>
    
</beans>