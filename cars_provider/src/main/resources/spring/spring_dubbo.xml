<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <!--<context:property-placeholder location="classpath:config.properties"/>-->

    <dubbo:application name="cars_provider" owner="xiyan" organization="cn.betaxiyan"/>
    <dubbo:provider executes="600" threads="600" retries="1" timeout="60000"/>
    <dubbo:protocol id="cars-protocol" name="dubbo" port="20086"/>

    <dubbo:registry id="cars-registry"  protocol="zookeeper" address="${zookeeper.url}" check="false" timeout="3000"/>

    <dubbo:service interface="com.xiyan.service.UserService" ref="userService" version="1.0.0"
                   group="${zookeeper.group}" registry="cars-registry" protocol="cars-protocol"/>
    
</beans>